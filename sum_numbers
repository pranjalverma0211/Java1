import java.util.Scanner;
class sum{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n,s;
        System.out.println("Enter the number");
        n=sc.nextInt();
        s=(n*(n-1))/2;
        System.out.println("Sum is:- "+s);
        

    }
}
