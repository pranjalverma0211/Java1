import java.util.Scanner;
class factorial{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a,c;
        int f=1;
        System.out.println("Enter a number");
        a = sc.nextInt();
        c=a;
        while(a!=0)
        {
            f = f*a;
            a=a-1;
        }
        System.out.println("Factorial of" +" "+c+" " +"is"+" " +f);
        

    }
}
